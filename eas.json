{
  "cli": {
    "version": ">= 3.20.0",
    "appVersionSource": "remote"
  },
  "build": {
    "apk": {
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "autoIncrement": true
      },
      "env": {
        "NPM_CONFIG_LEGACY_PEER_DEPS": "true",
        "EAS_BUILD_DISABLE_MAVEN_CACHE": "1"
      },
      "channel": "production"
    }
  }
}
